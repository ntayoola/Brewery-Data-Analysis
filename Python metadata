    "# Bar Chart of Average Quality Score by Beer Style\n",
    "\n",
    "# Sample 1000 data points for visualization\n",
    "df_sample = df.sample(n=1000, random_state=1)\n",
    "\n",
    "# Group by Beer Style and calculate the average quality score\n",
    "avg_quality_by_style = df_sample.groupby('Beer_Style')['Quality_Score'].mean().sort_values()\n",
    "\n",
    "# Plot Average Quality Score by Beer Style\n",
    "plt.figure(figsize=(12, 6))\n",
    "avg_quality_by_style.plot(kind='barh', color='skyblue')\n",
    "plt.title('Average Quality Score by Beer Style')\n",
    "plt.xlabel('Average Quality Score')\n",
    "plt.ylabel('Beer Style')\n",
    "plt.grid(True)\n",
    "plt.show()"
   ]
