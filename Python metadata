    "# Sample 1000 data points for visualization\n",
    "df_sample = df.sample(n=1000, random_state=1)\n",
    "\n",
    "# Group data by beer style and calculate the mean quality score\n",
    "quality_by_style = df_sample.groupby('Beer_Style')['Quality_Score'].mean().sort_values()\n",
    "\n",
    "# Plot bar chart\n",
    "plt.figure(figsize=(12, 6))\n",
    "quality_by_style.plot(kind='bar', color='skyblue')\n",
